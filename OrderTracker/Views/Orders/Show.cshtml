@{
  Layout = "_Layout";
      }

<h1>🛎️ Order for @Model["vendor"].Name </h1>

    <p><b>@Model["order"].Title</b></p>
    <p>@Model["order"].Date</p>
    <p>@Model["order"].Description</p>
    <p>@Model["order"].Price</p>

    <br>
    <br>
    <p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add another order</a></p>
    <p><a href='/vendors/@Model["vendor"].Id'>Back to orders for @Model["vendor"].Name </a></p>
    <p><a href='/vendors'>Back to vendors</a></p>
    <p><a href='/'>Back to home</a></p>

      